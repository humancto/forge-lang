// Benchmark: Factorial(20) called 10,000 times
fn factorial(n) {
    if n <= 1 {
        return 1
    }
    return n * factorial(n - 1)
}

let mut i = 0
let mut result = 0
while i < 10000 {
    result = factorial(20)
    i = i + 1
}
println("factorial(20) = {result}")
println("Computed 10000 times")
